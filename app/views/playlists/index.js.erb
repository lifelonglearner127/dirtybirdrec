$('.music-wrapper').addClass('loaded');
$('.playlist-nav-body li.active').removeClass('active')
$('#playlist-user').parent().addClass('active');

$('.music-wrapper').empty();
$('.music-wrapper').append('<div class="music-block"></div>')

<% if @playlists.present? %>
  $('.music-block').append(`<%= escape_javascript(render partial: 'playlists/list', locals: { playlists: @playlists }) %>`);
<% else %>
  $('.music-block').append(`<%= escape_javascript(render partial: 'playlists/no_list') %>`);
<% end %>