$('.music-wrapper').addClass('loaded');
$('.playlist-nav-body li.active').removeClass('active')
$('#playlist-favorited-tracks').parent().addClass('active');

$('.music-wrapper').empty();
$('.music-wrapper').append('<div class="music-block"></div>')

$('.music-block').append(`<%= render partial: 'playlists/page', locals: { tracks: @tracks, source_type: 'recently', source_id: @user.id } %>`);