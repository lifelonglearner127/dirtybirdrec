$('.music-wrapper').addClass('loaded');
$('.playlist-nav-body li.active').removeClass('active')
$('#playlist-favorites').parent().addClass('active');

$('.music-wrapper').empty();
$('.music-wrapper').append('<div class="music-block"></div>')

$('.music-block').append(`<%= render partial: 'favorites_content', locals: { user: @user, playlists: @playlists, tracks: @tracks, releases: @releases } %>`);