var tracks = [];
<% @tracks.each do |track| %>
  tracks.push({ 
    'title': '<%= track.title %>', 
    'artists': '<%= track.users.pluck(:name).join(' feat. ') %>', 
    'mp3': '<%= track.get_url %>' 
  });
<% end %>

setPlaylist(tracks,true);


$("[id^='play-release-']").show().next('.jp-controls').css({'display': 'none'});
$('#play-release-<%= @release.id %>').hide().next('.jp-controls').css({'display': 'inline-block'});