  <%# if track.users.any? %>
    <%# artists = track.users.map(&:name).join(' feat. ') %>
  <%# else %>
    <%# artists = track.artist %>
  <%# end %>

  tracks.push({ 
    'release_id': '<%= track.release.id %>',
